{
  "name": "enginedigital/laravel-netsuite",
  "description": "A laravel5 wrapper around ryanwinchester/netsuite-php",
  "require": {
        "laravel/framework": "^5",
        "ryanwinchester/netsuite-php": "^2016.1|^v2017|^v2018",
        "guzzlehttp/guzzle": "^6.2"
    },
  "require-dev": {
      "roave/security-advisories": "dev-master"
  },
  "replace": {
      "paragonie/random_compat": "*"
  },
  "autoload": {
      "psr-4": {
          "EngineDigital\\NetSuite\\": "src"
      }
  },
  "license": "MIT",
  "extra": {
      "laravel": {
          "providers": [
              "EngineDigital\\NetSuite\\Providers\\NetSuiteServiceProvider",
              "EngineDigital\\NetSuite\\Providers\\NetSuiteApiProvider"
          ]
      }
  }
}
